            <length>0.025</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <link name="rear_right_wheel_rotor">
      <pose>-0.5 -0.5 -0.275 1.57 0 0</pose>
      <inertial>
        <mass>5.0</mass>
        <inertia>
          <ixx>0.0029167</ixx>
          <iyy>0.0029167</iyy>
          <izz>0.0056250</izz>
        </inertia>
      </inertial>
      <visual name="rr_wheel_visual">
        <geometry>
          <cylinder>
            <radius>0.2</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.2 0.2 0.2 1.0</ambient>
          <diffuse>0.2 0.2 0.2 1.0</diffuse>
        </material>
      </visual>
      <collision name="rr_wheel_collision">
        <geometry>
          <cylinder>
            <radius>0.2</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0.8</mu>
              <mu2>0.8</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
    </link>

    <!-- Rear Right Joints -->
    <joint name="rear_right_wheel_joint" type="fixed">
      <parent>body</parent>
      <child>rear_right_wheel_base_link</child>
    </joint>

    <joint name="rear_right_wheel_suspension_joint" type="prismatic">
      <parent>rear_right_wheel_base_link</parent>
      <child>rear_right_wheel_suspension</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-0.02</lower>
          <upper>0.02</upper>
          <effort>100</effort>
          <velocity>1.0</velocity>
        </limit>
        <dynamics>
          <damping>0.1</damping>
          <spring_stiffness>50000</spring_stiffness>
          <spring_reference>0</spring_reference>
        </dynamics>
      </axis>
    </joint>

    <joint name="rear_right_wheel_steer_joint" type="revolute">
      <parent>rear_right_wheel_suspension</parent>
      <child>rear_right_wheel_steer</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-3.14159</lower>
          <upper>3.14159</upper>
          <effort>100</effort>
          <velocity>10</velocity>
        </limit>
      </axis>
    </joint>

    <joint name="rear_right_wheel_rotor_joint" type="revolute">
      <parent>rear_right_wheel_steer</parent>
      <child>rear_right_wheel_rotor</child>
      <axis>
        <xyz>0 0 -1</xyz>
        <dynamics>
          <damping>0.1</damping>
        </dynamics>
      </axis>
    </joint>

